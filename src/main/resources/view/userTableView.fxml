<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.pwn.piotr.bazafx.controller.UserTableController">
   <children>
      <TableView fx:id="userTable" layoutY="60.0" prefHeight="272.0" prefWidth="601.0">
        <columns>
          <TableColumn fx:id="colId" prefWidth="54.0" text="Id" />
          <TableColumn fx:id="colLogin" prefWidth="217.0" text="Login" />
            <TableColumn fx:id="colRole" prefWidth="173.0" text="Role" />
            <TableColumn fx:id="colActive" prefWidth="154.0" text="Active" />
        </columns>
      </TableView>
      <Button fx:id="btnBack" layoutX="14.0" layoutY="347.0" mnemonicParsing="false" onMouseClicked="#backAction" text="&lt;&lt; Back" />
      <Button fx:id="btnDeleteUser" layoutX="183.0" layoutY="347.0" mnemonicParsing="false" onMouseClicked="#deleteUser" text="Delete user" />
   </children>
</AnchorPane>
